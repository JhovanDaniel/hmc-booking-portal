<%= turbo_frame_tag "appointment-reports" do %>
  <% if defined?(@report_type) && defined?(@start_date) && defined?(@end_date) %>
    <% if @report_type == 'Appointment Data' %>
      <%= render "reports/appointment_data" %>
    <% end %>
    
    <% if @report_type == 'Procedure Data' %>
      <%= render "reports/procedure_data" %>
    <% end %>
    
    <% if @report_type == 'Payment Data' %>
      <%= render "reports/payment_data" %>
    <% end %>
  <% end %>
<% end %>